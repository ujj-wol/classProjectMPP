<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="450.0" prefWidth="495.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.AddAuthorController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="17.0" prefWidth="227.0" text="Add Author" />
      <TextField fx:id="authorFname" layoutX="19.0" layoutY="52.0" prefHeight="27.0" prefWidth="227.0" promptText="author first name" />
      <TextField fx:id="authorLname" layoutX="19.0" layoutY="88.0" prefHeight="27.0" prefWidth="227.0" promptText="author last name" />
      <TextField fx:id="authorPhone" layoutX="19.0" layoutY="128.0" prefHeight="27.0" prefWidth="227.0" promptText="author phone" />
      <TextField fx:id="street" layoutX="255.0" layoutY="52.0" prefHeight="27.0" prefWidth="227.0" promptText="street address" />
      <TextField fx:id="city" layoutX="255.0" layoutY="88.0" prefHeight="27.0" prefWidth="227.0" promptText="city" />
      <TextField fx:id="state" layoutX="255.0" layoutY="128.0" prefHeight="27.0" prefWidth="227.0" promptText="state" />
      <TextField fx:id="zip" layoutX="255.0" layoutY="167.0" prefHeight="27.0" prefWidth="227.0" promptText="zip" />
      <TextArea fx:id="authorBio" layoutX="16.0" layoutY="225.0" prefHeight="91.0" prefWidth="464.0" promptText="add author bio" />
      <Button layoutX="140.0" layoutY="355.0" mnemonicParsing="false" onAction="#cancelAuthor" prefHeight="27.0" prefWidth="115.0" text="Cancel" />
      <Button layoutX="16.0" layoutY="355.0" mnemonicParsing="false" onAction="#submitAuthor" prefHeight="27.0" prefWidth="115.0" text="Submit" />
   </children>
</AnchorPane>
